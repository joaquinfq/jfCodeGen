{{#each methods}}
    /**
    {{#if desc}}
        {{#each desc}}
            * {{{tr this}}}
        {{/each}}
        {{#if override}}
            * {{{tr "MÃ©todo heredado de `%s`." @root/base}}}
        {{/if}}
        *
    {{/if}}
    * @method {{name}}
    {{#if params}}
        *
        {{#each params}}
            * @param {{doc-type type}} {{name}} {{{desc}}}
        {{/each}}
    {{/if}}
    {{#if returns}}
        ** @return {{returns}}
    {{/if}}
    *
    {{scope name}}
    */
    {{name}}({{#each params}}{{name}}{{#unless @last}}, {{/unless}}{{/each}}){
    {{#if before}}
        super.{{name}}({{#each params}}{{name}}{{#unless @last}}, {{/unless}}{{/each}});
    {{/if}}
    {{{body}}}
    {{#if after}}
        {{#if returns}}
            return super.{{name}}({{#each params}}{{name}}{{#unless @last}}, {{/unless}}{{/each}});
        {{else}}
            super.{{name}}({{#each params}}{{name}}{{#unless @last}}, {{/unless}}{{/each}});
        {{/if}}
    {{/if}}
    }
{{/each}}
