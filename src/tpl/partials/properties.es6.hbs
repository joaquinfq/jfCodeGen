{{#each properties}}
    /**
    {{#if desc}}
        {{#each desc}}
            {{#if this}}
                * {{{tr this}}}
            {{else}}
                *
            {{/if}}
        {{/each}}
        {{#if override}}
            * {{{tr "Propiedad heredada de `%s`." @root/base}}}
        {{/if}}
        *
    {{/if}}
    * @property {{name}}
    {{#if type}}
        * @type     {{doc-type type}}
    {{/if}}
    *
    {{scope name}}
    {{#if readonly}}
        * @final
    {{/if}}
    */this.{{name}} = {{{indent this 4}}};
{{/each}}
